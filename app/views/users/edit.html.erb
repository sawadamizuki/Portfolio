<h2>プロフィール編集</h2>
<%= form_with model:@user, local:true do |f| %>
  <label for="inputName">ユーザー名</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"名前"%>
  <%= attachment_image_tag @user, :profile_image, :fill, 60, 60 %>
  <label for="inputImage">アイコン</label>
  <%= f.attachment_field :profile_image, placeholder: "アイコン" %>
  <label for="inputImage">自己紹介</label>
  <%= f.text_field :introduction %>
  <%= f.submit "変更を保存" %>
<% end %>